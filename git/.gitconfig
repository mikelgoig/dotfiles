[color]
	branch = auto
	diff = auto
	status = auto
	ui = always

[core]
	attributesfile = ~/.gitattributes
	autocrlf = input
	excludesfile = ~/.gitignore_global
	ignorecase = false
	precomposeunicode = false
	trustctime = false

[diff]
	compactionHeuristic = true
	indentHeuristic = true

[fetch]
	prune = true

[filter "lfs"]
	clean = git-lfs clean -- %f
	process = git-lfs filter-process
	required = true
	smudge = git-lfs smudge -- %f

[help]
	autocorrect = 1

[init]
	defaultBranch = master

[push]
	default = simple

[user]
	email = m.goig@docline.com
	name = Mikel Goig
